{
  "name": "Mentor",
  "version": "0.1.0",
  "private": true,
  "graphql-endpoint": "https://www.virtualmentor.io/graphql",
  "scripts": {
    "clean": "watchman watch-del-all && rm -rf node_modules && npm cache clear && rm -rf $TMPDIR/react-*",
    "clean:babel": "find node_modules -type f -name .babelrc | grep -v /react-native/ | xargs rm",
    "build:schema": "node tools/build-graphql-schema.js",
    "postinstall": "npm run clean:babel && npm run build:schema",
    "start:app": "node node_modules/react-native/local-cli/cli.js --port 8081 --reset-cache",
    "start": "NODE_ENV=development TYPE_ENV=development npm run build:schema && npm run start:app",
    "build:prod": "react-native bundle --entry-file index.ios.js --bundle-output ios/main.jsbundle --minify=true --platform=ios --assets-dest=./ --dev=false"
  },
  "dependencies": {
    "babel-relay-plugin": "0.8.1",
    "events": "^1.1.0",
    "graphql": "^0.5.0",
    "graphql-relay": "^0.4.1",
    "lodash": "4.6.1",
    "moment": "^2.11.1",
    "path": "^0.12.7",
    "react-native": "0.24.1",
    "react-native-clipboard": "0.0.5",
    "react-native-device-info": "0.9.3",
    "react-native-fbsdkcore": "0.0.8",
    "react-native-fbsdklogin": "0.0.8",
    "react-native-in-app-utils": "^3.0.0",
    "react-native-linear-gradient": "^1.5.2",
    "react-native-progress": "2.2.2",
    "react-native-swipeout": "git+https://bitbucket.org/DenisIzmaylov/react-native-swipeout2.git",
    "react-native-vector-icons": "1.2.0",
    "react-redux": "4.4.5",
    "react-relay": "0.8.1",
    "redux": "3.5.2",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^1.0.3",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "babel-preset-react-native": "^1.6.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flow-vars": "^0.2.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "react": "^0.14.7",
    "react-native-cli": "^0.2.0",
    "source-map-support": "^0.4.0",
    "sync-request": "^3.0.1"
  }
}
