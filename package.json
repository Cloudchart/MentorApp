{
  "name": "Mentor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "find node_modules -type f -name .babelrc | grep -v /react-native/ | xargs rm",
    "startApp": "node_modules/react-native/packager/packager.sh --port 8081",
    "start": "NODE_ENV=development TYPE_ENV=development npm run startApp",
    "buildIOS": "react-native bundle --entry-file index.ios.js --bundle-output ios/main.jsbundle --minify=true --platform=ios --assets-dest ./ --dev=false"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "events": "^1.1.0",
    "graphql": "^0.4.14",
    "lodash": "^4.2.0",
    "moment": "^2.11.1",
    "path": "^0.12.7",
    "react-native": "0.20.0",
    "react-native-clipboard": "0.0.5",
    "react-native-device-info": "^0.8.3",
    "react-native-fbsdkcore": "0.0.8",
    "react-native-fbsdklogin": "0.0.8",
    "react-native-linear-gradient": "^1.5.2",
    "react-native-progress": "2.2.0",
    "react-native-swipeout": "git+https://bitbucket.org/DenisIzmaylov/react-native-swipeout2.git",
    "react-native-vector-icons": "1.2.0",
    "react-redux": "4.4.0",
    "redux": "3.2.1",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^1.0.3",
    "url": "^0.11.0"
  },
  "devDependencies": {
